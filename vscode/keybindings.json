[
  // Navigation
  {
    "key": "ctrl-h",
    "command": "workbench.action.navigateLeft"
  },
  {
    "key": "ctrl-l",
    "command": "workbench.action.navigateRight"
  },
  {
    "key": "ctrl-k",
    "command": "workbench.action.navigateUp"
  },
  {
    "key": "ctrl-j",
    "command": "workbench.action.navigateDown"
  },
  {
    "key": "space ,",
    "command": "workbench.action.showAllEditors",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space e",
    "args": {
      "commands": [
        "workbench.action.toggleSidebarVisibility",
        "workbench.files.action.focusFilesExplorer"
      ]
    },
    "command": "runCommands",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !sideBarFocus && !terminalFocus"
  },
  {
    "key": "space e",
    "args": {
      "commands": [
        "workbench.action.toggleSidebarVisibility",
        "workbench.action.focusActiveEditorGroup"
      ]
    },
    "command": "runCommands",
    "when": "sideBarFocus && !inputFocus && !terminalFocus"
  },
  {
    "key": "space e",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "vim.mode == 'Normal' && editorTextFocus && foldersViewVisible"
  },
  {
    "key": "space z c",
    "command": "editor.foldAll",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && gitOpenRepositoryCount > 0 && !inQuickOpen"
  },
  {
    "key": "space z o",
    "command": "editor.unfoldAll",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && gitOpenRepositoryCount > 0 && !inQuickOpen"
  },
  {
    "key": "shift+l",
    "command": "workbench.action.nextEditorInGroup",
    "when": "(vim.mode == 'Normal' || vim.mode == 'Visual') && (editorTextFocus || !inputFocus)"
  },
  {
    "key": "shift+h",
    "command": "workbench.action.previousEditorInGroup",
    "when": "(vim.mode == 'Normal' || vim.mode == 'Visual') && (editorTextFocus || !inputFocus)"
  },
  {
    "key": "space g d",
    "command": "gitlens.diffWithPrevious",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && gitOpenRepositoryCount > 0 && !inQuickOpen"
  },
  {
    "key": "space g h",
    "command": "gitlens.showQuickFileHistory",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) &&  gitOpenRepositoryCount > 0"
  },
  {
    "key": "space g t",
    "command": "gitlens.showRepositoriesView",
    "when": "vim.mode == 'Normal' &&  (editorTextFocus || !inputFocus) && gitOpenRepositoryCount > 0"
  },
  {
    "key": "space g b",
    "command": "gitlens.showBranchesView",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && gitOpenRepositoryCount > 0"
  },
  // Coding
  {
    "key": "shift k",
    "command": "editor.action.showHover",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "shift-j",
    "command": "editor.action.moveLinesDownAction",
    "when": "vim.mode == 'VisualLine' && editorTextFocus"
  },
  {
    "key": "shift-k",
    "command": "editor.action.moveLinesUpAction",
    "when": "vim.mode == 'VisualLine' && editorTextFocus"
  },
  {
    "key": "shift-k",
    "command": "editor.action.showHover",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  // Refactoring
  {
    "key": "space r r",
    "command": "editor.action.rename",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space r m",
    "args": {
      "kind": "refactor.extract.method"
    },
    "command": "editor.action.codeAction",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space r v",
    "args": {
      "kind": "refactor.extract.variable"
    },
    "command": "editor.action.codeAction",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space r c",
    "args": {
      "kind": "refactor.extract.constant"
    },
    "command": "editor.action.codeAction",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space r s",
    "command": "editor.action.showHover",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space r a",
    "command": "editor.action.refactor",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space s a",
    "command": "editor.action.codeAction",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space s f",
    "command": "editor.action.formatDocument",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space c",
    "command": "workbench.action.closeActiveEditor",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space space",
    "command": "workbench.action.quickOpen",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus)"
  },
  {
    "key": "space s m",
    "command": "workbench.action.gotoSymbol",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space s d",
    "command": "editor.action.revealDefinition",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus)"
  },
  {
    "key": "space s u",
    "command": "editor.action.goToReferences",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "gr",
    "command": "editor.action.referenceSearch.trigger",
    "when": "editorTextFocus"
  },

  {
    "key": "gd",
    "command": "editor.action.revealDefinition",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "gD",
    "command": "editor.action.revealDeclaration",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "gr",
    "command": "editor.action.goToReferences",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "gi",
    "command": "editor.action.goToImplementation",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "gm",
    "command": "workbench.action.gotoSymbol",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
    {
    "key": "space f",
    "command": "workbench.action.findInFiles",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space f f",
    "command": "workbench.action.quickOpen",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space f r",
    "command": "workbench.action.openRecent",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space n",
    "command": "workbench.action.files.newUntitledFile",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "ctrl-n",
    "command": "editor.action.SelectionToNextFindMatch",
    "when": "(vim.mode == 'Normal' || vim.mode == 'Visual') && (editorTextFocus || !inputFocus)"
  },
  // File Explorer
  {
    "key": "r",
    "command": "renameFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "c",
    "command": "filesExplorer.copy",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "p",
    "command": "filesExplorer.paste",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "x",
    "command": "filesExplorer.cut",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "d",
    "command": "deleteFile",
  "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "a",
    "command": "explorer.newFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "shift-a",
    "command": "explorer.newFolder",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "s",
    "command": "explorer.openToSide",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "shift-s",
    "args": {
      "commands": [
        "workbench.action.splitEditorDown",
        "explorer.openAndPassFocus",
        "workbench.action.closeOtherEditors"
      ]
    },
    "command": "runCommands",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "space g g",
    "args": {
      "text": "wt -d ${fileDirname} lazygit\r"
    },
    "command": "workbench.action.terminal.sendSequence",
    "when": "filesExplorerFocus && foldersViewVisible && !inputFocus"
  },
  {
    "key": "enter",
    "command": "list.toggleExpand",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && explorerResourceIsFolder && !inputFocus"
  },
  {
    "key": "space t t",
    "args": {
      "cwd": "${fileDirname}"
    },
    "command": "workbench.action.terminal.newWithCwd",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "key": "\\|",
    "command": "workbench.action.splitEditorRight",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "\\\\",
    "command": "workbench.action.splitEditorDown",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space w m",
    "command": "workbench.action.moveEditorToNextGroup",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space w u",
    "command": "workbench.action.closeActiveEditor",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space",
    "command": "-",
    "when": "vim.mode == 'Insert' && editorTextFocus"
  },
  {
    "key": "ctrl+shift+p",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "space s c",
    "command": "workbench.action.showCommands",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space shift+7",
    "command": "editor.action.commentLine",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  {
    "key": "space shift+7",
    "command": "editor.action.commentLine",
    "when": "editorTextFocus && vim.mode == 'Visual'"
  },
  {
    "key": "space shift+7",
    "command": "editor.action.commentLine",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && !terminalFocus"
  },
  //default ctrl +  mappings von vim
 {
      "key": "ctrl+b",
      "command": "scrollPageUp",
      "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
      "key": "ctrl+d",
      "command": "scrollPageDown",
      "when": "editorTextFocus && vim.active && vim.mode == 'Normal'" 
  },
  {
      "key": "ctrl+e",
      "command": "scrollLineUp",
      "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
      "key": "ctrl+f",
      "command": "actions.find",
      "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
      "key": "ctrl+w",
      "command": "vim.remap",
      "when": "editorTextFocus && vim.mode != 'Insert'"
  },
  {
      "key": "ctrl+s v",
      "command": "workbench.action.splitEditorRight",
      "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
      "key": "ctrl+s h",
      "command": "workbench.action.splitEditorDown",
      "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
      "key": "ctrl+w q",
      "command": "workbench.action.closeActiveEditor",
      "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
      "key": "ctrl+w m",
      "command": "workbench.action.moveEditorToNextGroup",
      "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
      "key": "ctrl+w shift+m",
      "command": "workbench.action.moveEditorToPreviousGroup",
      "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+s",
    "command": "workbench.action.files.save",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+n",
    "command": "editor.action.addSelectionToNextFindMatch",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+ctrl+n",
    "command": "cursorUndo",
    "when": "editorTextFocus"
  }


]
